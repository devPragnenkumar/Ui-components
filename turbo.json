{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*",
        "storybook/.storybook/**",
        "storybook/src/**",
        "storybook/package.json",
        "!storybook-static/**"
      ],
      "outputs": ["dist/**" ,"storybook-static/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
